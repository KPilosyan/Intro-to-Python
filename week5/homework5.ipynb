    "def my_range(n):\n",
    "    i=0\n",
    "    while i <= n:\n",
    "        yield i\n",
    "        i+=1\n",
    "    if i>n:\n",
    "        print(\"There are no values left\")\n",
    "    \n",
    "x = my_range(4)"
